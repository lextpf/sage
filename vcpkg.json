    "qtdeclarative",
    "qtquickcontrols2",
    "gtest",
    "opencv"
  ],
  "overrides": [
    {
      "name": "openssl",
      "version": "3.4.1"
    }
  ],
  "builtin-baseline": "aa2d37682e3318d93aef87efa7b0e88e81cd3d59"
